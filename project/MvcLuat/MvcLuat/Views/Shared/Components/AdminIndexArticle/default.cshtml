@{
}
@model IEnumerable<MvcLuat.Models.Article>
@foreach(var item in Model)
{
    <div class="noi_dung">
            <div class="tach">
                    <a class="print_dieu" href="/Admin?id=@(item.ID + 5)">@item.Title</a>
                    <div class="box_box">
                    <a class="but_hover edit_but" href="/Admin/ArticleEdit/@item.ID">Chỉnh sửa</a>
                    <!-- <br> -->
                    <a class="but_hover delete_but" href="/Admin/ArticleDelete/@item.ID">Xóa</a>
                </div>
            </div>
    </div>
}